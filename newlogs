::: Array
(
    [newcontent] => Model.Root={DocBody}

DocBody={Sec}

Doc=<font color="grey">{MessageToUser}</font><center><h4>{Head.Sec}</h4></center><br>{sec} 

Sec=<b>{Ti}</b><br>{sec}

sec={0.sec} {xlist} {90.sec}

xlist={Olist}

Olist=<{ol}><li>{Secs}</li></ol>

olist=<{ol}><li>{secs}</li></ol>

ol=ol

ol-a=ol type="a"

ol-A=ol type="A"

ol-I=ol type="I"

ol-i=ol type="i"


